{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "title": "Tesla Motors Daily Stock Price",
  "data": {
    "url": "https://www.quandl.com/api/v3/datasets/WIKI/TSLA.csv"
  },
  "mark": "line",
  "encoding": {
    "x": {
      "field": "Date",
      "type": "temporal",
      "axis": {"title": false}
    },
    "y": {
      "field": "Close",
      "type": "quantitative",
      "axis": {"title": "Closing Price"}
    }
  }
}
